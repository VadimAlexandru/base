{
    "name": "wtg-package/base",
    "type": "library",
    "description": "wtg pkg",
    "keywords": [
        "wtg"
    ],
    "homepage": "https://github.com/x/base",
    "license": "proprietary",
    "authors": [
        {
            "name": "VadimAleksandru",
            "email": "v700.lg@gmail.com",
            "homepage": "https://github.com/VadimAlexandru",
            "role": "Creator & Maintainer"
        }
    ],
    "require": {
        "almasaeed2010/adminlte" : "2.4.*",
        "laravel/framework": "^7.0",
        "prologue/alerts": "^0.4.1",
        "creativeorange/gravatar": "~1.0",
        "nesbot/carbon": "^2.14.0",
        "laravel/helpers": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "backpack/generators": "^1.1",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Backpack\\Base\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backpack\\Base\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Backpack\\Base\\BaseServiceProvider"
            ]
        }
    }
}
